#include <stdio.h>
#include <string.h>
#include "arena_alloc.h"

c
int main(void){
  arena ar = arena_new(500);
  for (int i = 0; i < 10; i++){
    char *str = arena_alloc(&ar, 50);
    if (str != NULL){
     strcpy(str, "This is a cool string");
    }
    else{
      puts("The arena is full");
    }
  }
  printf("%ld,%ld\n",sizeof(arena),sizeof(garena));
  arena_free(&ar);
  return 0; 
}
